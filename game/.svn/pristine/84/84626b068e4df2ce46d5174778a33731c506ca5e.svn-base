#benefictDetails {
	display: none;
}

.interest-point-detail-img {
	height: auto;
	width: 100%;
}

input.filter-box {
	float: left;
	margin-left: 19%;
	border-radius: 5px;
	padding: 4px;
	width: 90%;
}

.btnSelectBenefi {
	display: inline-block;
	position: relative;
	float: left;
	margin-left: -14%;
	width: 100%;
	margin-bottom: 20px;
}

.btnSelectBenefi .dropdown-menu {
	background: white !important;
}

.btnSelectBenefi div {
	float: left;
	margin-left: 50px;
	position: relative;
}

img#imgBenefitst {
	margin: 20px;
}

/*Estilos beneficios*/
div#benefictDetails {
	width: 100%;
	color: black;
}

.IP-list-name {
	display: none;
}

span.glyphicons.circle_arrow_left.backDetails {
	float: left;
	bottom: 42px;
	left: 10px;
	cursor: pointer;
}

li.benefictElement img {
	border-bottom: 8px solid #C4120C;
	height: auto;
	width: 38%;
	position: absolute;
	border: 1px solid rgba(196, 18, 12, 0.11);
	padding: 10px;
	border-radius: 15px;
	cursor: pointer;
}

span.glyphicons.circle_arrow_left:before {
	color: rgb(209, 12, 26);
	font: 33px/1em 'Glyphicons Regular';
}

img#imgBenefitst {
	height: 63px;
	width: 50%;
	margin-left: auto;
	margin-right: auto;
	position: relative;
}

.interest-point-details-body ul li {
	list-style: none;
}

.interest-point-details-body ul {
	padding-left: 0px;
}

.containerTemplete {
	padding: 0px;
}

span.details-label, .details-second-label {
	font-weight: bold;
	margin-right: 10px;
}

.interest-point-details-body {
	padding-right: 10px;
}

.interest-point-detail-img {
	border-bottom: 8px solid #C4120C;
}

.contImgsBenefi {
	clear: both;
}

.grid.cs-style-1 {
	margin-left: 0px !important;
	margin-top: 0px !important;
	padding-bottom: 0px;
}

.grid figure img {
	max-width: 100%;
	display: block;
	position: relative;
	height: auto;
	width: 90%;
	margin-left: auto;
	margin-right: auto;
	-webkit-box-shadow: 11px 11px 26px -8px rgba(0, 0, 0, 0.75);
	-moz-box-shadow: 11px 11px 26px -8px rgba(0, 0, 0, 0.75);
	box-shadow: 7px 6px 30px -13px rgba(0, 0, 0, 0.75);
}

.grid li {
	display: inline-block;
	width: 33%;
	margin: -1px;
	padding: 20px;
	text-align: left;
	position: relative;
	margin-bottom: 30px !important;
}

@media screen and (max-width: 25em) {
	.codrops-icon span {
		display: none;
	}
}

@media only screen and (min-width : 480px) and (max-width : 767px) {
	.grid li {
		padding: 0px !important;
		margin-bottom: 30px !important;
	}
	img#imgBenefitst {
	    height: 80px;
    	width: auto;
	}
	#benefictDetails .interest-point-detail-img {
		width: 90%;
		margin: 0 auto;
		border: 1px solid rgba(196, 18, 12, 0.11);
		padding: 10px;
		border-radius: 15px;
		border-bottom: 8px solid #C4120C;
		position: inherit;
	}
}

@media only screen and (min-width : 768px) and (max-width : 991px) {
	.grid li {
		padding: 0px !important;
		margin-bottom: 30px !important;
	}
	img#imgBenefitst {
		height: 80px;
		width: 30%;
	}
}

@media only screen and (max-width: 1199px) and (min-width: 768px) {
	div#benefictDetails img {
    height: 100%;
	}
	.grid li {
		padding: 0px !important;
		margin-bottom: 30px !important;
	}
	img#imgBenefitst {
		height: 90px;
		width: 30%;
	}
	.containerTemplete {
		padding: 30px;
	}
	#benefictDetails .interest-point-detail-img {
		height: 240px;
		width: 300px;
		position: absolute;
		left: 60%;
		top: 40%;
		border: 1px solid rgba(196, 18, 12, 0.11);
		padding: 10px;
		border-radius: 15px;
		border-bottom: 8px solid #C4120C;
	}
}

@media only screen and (min-width : 1200px) {
	.interest-point-detail-img {
		height: 220px;
	}
	div#benefictDetails img {
    height: 100%;
}
	.interest-point-details-body ul li {
		font-size: 14px;
	}
	.interest-point-details-body {
		width: 65%;
	}
	input.filter-box {
		margin-left: 5%;
		width: 33%;
	}
	img#imgBenefitst {
		height: 120px;
		top: 0px;
		width: 35%;
	}
	.grid li {
		width: 24%;
		margin: 0;
		padding: 0px !important;
	}
	.grid.cs-style-1 {
		margin-left: 3% !important;
	}
	#benefictDetails .interest-point-detail-img {
		height: 240px;
		width: 300px;
		position: absolute;
		left: 60%;
		top: 40%;
		border: 1px solid rgba(196, 18, 12, 0.11);
		padding: 10px;
		border-radius: 15px;
		border-bottom: 8px solid #C4120C;
	}
}