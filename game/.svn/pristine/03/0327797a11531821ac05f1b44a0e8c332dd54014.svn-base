package cr.fi.grupomutual.website.entities;

import com.fasterxml.jackson.annotation.JsonIgnoreProperties;
import com.fasterxml.jackson.annotation.JsonProperty;

/**
 * Class to map the value of json object data from the web service 
 * @author hcampos-as Avantica Technologies
 */
@JsonIgnoreProperties(ignoreUnknown = true)
public class JsonValue {
	
	@JsonProperty("@xmlns:v1")
	private String xmlns;	
	@JsonProperty("$")
	private String value;
	
	// getters
	public String getXmlns(){ return xmlns; }
	public String getValue(){ return value; }
	
}
