package cr.fi.grupomutual.website.services;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;

import cr.fi.grupomutual.website.entities.PromotionsWsResponse.PromotionResponse.Promotion;
import cr.fi.grupomutual.website.entities.locations.Canton;
import cr.fi.grupomutual.website.entities.locations.District;
import cr.fi.grupomutual.website.entities.locations.Category.ConsultarCategoriasPuntoDeInteresMsjRespTipo.ListaCategoriaPuntoInteres;
import cr.fi.grupomutual.website.entities.locations.InterestPoints.ConsultarPuntosDeInteresMsjRespTipo.ListaPuntoInteres;
import cr.fi.grupomutual.website.entities.locations.Province;
import cr.fi.grupomutual.website.ws.NearMeWsClient;

public class NearMeServiceImpl implements NearMeService{

	@Autowired
	NearMeWsClient nearMeWsClient;

	public Canton getCantonsData(String provinceId) {
		return nearMeWsClient.getCantonsData(provinceId);

	}

	public District getDistrictsData(String cantonId, String provinceId) {
		return nearMeWsClient.getDistrictsData(cantonId, provinceId);
	}

	public List<ListaPuntoInteres> getInterestPoints(String latitude,
			String longitude) {
		return nearMeWsClient.getInterestPoints(latitude, longitude);
	}

	public List<ListaPuntoInteres> getInterestPoints(String provinceId,
			String cantonId, String districtId) {
		return nearMeWsClient.getInterestPoints(provinceId, cantonId,
				districtId);
	}

	public List<ListaPuntoInteres> getPromotions(String latitude,
			String longitude) {
		return nearMeWsClient.getPromotions(latitude, longitude);
	}

	public Province getProvincesData() {
		return nearMeWsClient.getProvincesData();
	}
	public List<ListaCategoriaPuntoInteres> getCategoriesData(){
		return nearMeWsClient.getCategoriesData();
		
	}
	
	
	public List<ListaPuntoInteres> getMutualBenefict(){
		return nearMeWsClient.getMutualBenefict();
	}
	
	public List<Promotion> getPromotions() throws RuntimeException{
		return nearMeWsClient.getPromotions();
	}
}
