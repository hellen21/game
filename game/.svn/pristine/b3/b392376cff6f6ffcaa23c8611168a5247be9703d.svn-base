package cr.fi.grupomutual.website.entities;

import java.util.List;
import com.fasterxml.jackson.annotation.JsonIgnoreProperties;
import com.fasterxml.jackson.annotation.JsonProperty;

public class ContactResponse {
	@JsonProperty("ConsultarContactoMsjRespTipo")
	private ContactResponseWS consultSectionContentMsjSolTipo;
	
	public ContactResponseWS getConsultSectionContentMsjSolTipo() {
		return consultSectionContentMsjSolTipo;
	}

	public void setConsultSectionContentMsjSolTipo(
			ContactResponseWS consultSectionContentMsjSolTipo) {
		this.consultSectionContentMsjSolTipo = consultSectionContentMsjSolTipo;
	}

	@JsonIgnoreProperties(ignoreUnknown = true)
	public static class ContactResponseWS extends HeaderResponse {

		@JsonProperty("Contacto")
	
		private List<Contacts> contact;

		public List<Contacts> getContact() {
			return contact;
		}

		public void setContact(List<Contacts> contact) {
			this.contact = contact;
		}
		@JsonIgnoreProperties(ignoreUnknown = true)
		public static class Contacts {
			@JsonProperty("IdContacto")
			private String idContact;
			@JsonProperty("IdCanal")
			private String idCanal;
			@JsonProperty("TipoContacto")
			private String contactType;
			@JsonProperty("Correos")
			private List<String> emails;
			
			public String getIdContact() {
				return idContact;
			}
			public void setIdContact(String idContact) {
				this.idContact = idContact;
			}
			public String getIdCanal() {
				return idCanal;
			}
			public void setIdCanal(String idCanal) {
				this.idCanal = idCanal;
			}
			public String getContactType() {
				return contactType;
			}
			public void setContactType(String contactType) {
				this.contactType = contactType;
			}
			public List<String> getEmails() {
				return emails;
			}
			public void setEmails(List<String> emails) {
				this.emails = emails;
			}
		}
	}
}
