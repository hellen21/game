package cr.fi.grupomutual.website.entities.locations;

import java.util.List;

import com.fasterxml.jackson.annotation.JsonIgnoreProperties;
import com.fasterxml.jackson.annotation.JsonProperty;

import cr.fi.grupomutual.website.entities.HeaderResponse;


/**
 * Class to store categories data from the web service
 * @author cvargas-as Avantica Technologies
 */
@JsonIgnoreProperties(ignoreUnknown = true)
public class Category {
	
	@JsonProperty("ConsultarCategoriasPuntoDeInteresMsjRespTipo")
	private ConsultarCategoriasPuntoDeInteresMsjRespTipo consultarCategoriasPuntoDeInteresMsjRespTipo;
	
	// getters
	public ConsultarCategoriasPuntoDeInteresMsjRespTipo getConsultarCategoriasPuntoDeInteresMsjRespTipo(){ return consultarCategoriasPuntoDeInteresMsjRespTipo; }
	
	@JsonIgnoreProperties(ignoreUnknown = true)
	public static class ConsultarCategoriasPuntoDeInteresMsjRespTipo extends HeaderResponse{
		@JsonProperty("ListaCategoriaPuntoInteres")
		private List<ListaCategoriaPuntoInteres> listaCategoriaPuntoInteres;

		// getters
		public List<ListaCategoriaPuntoInteres> getListaCategoriaPuntoInteres(){ return listaCategoriaPuntoInteres; }
		
		@JsonIgnoreProperties(ignoreUnknown = true)
		public static class ListaCategoriaPuntoInteres {
			@JsonProperty("Codigo") private String code;
			@JsonProperty("Descripcion") private String description;
			@JsonProperty("Color") private String color;
			@JsonProperty("Estatus") private String status;
			
			// getters
			public String getCode() { return code; }
			public String getDescription() { return description; }
			public String getColor() { return color; }
			public String getStatus() { return status; }
			
		}
	}   
}
