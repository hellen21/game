<footer class="bottom">

	<div class="colorGrayFooter">
		<div id="fooCarouselDiv"></div>
	</div>
	
	<div class="container">
		<div class="row">
			<div class="container">
				<div id="footerSection1" class="col-sm-6">
					<ol class="olFooter">
						<li th:each="itemFooter : ${itemList}" ><a th:href="@{'/'+${#strings.replace(itemFooter.title, ' ', '')} +'/'+${itemFooter.getCode()}}" th:text="${itemFooter.title}"></a></li>
					
					</ol>
				</div>
				<div id="footerLinks" class="col-sm-6">
					<ol class="olFooterMenu">
						<li><a class="start" th:href="@{/}" th:text="#{label.footer.rightOne}"></a></li>
						<li><a th:href="@{/mapaDelSitio}" th:text="#{label.footer.rightTwo}"></a></li>
						<li><a th:href="@{/contactenos}" th:text="#{label.footer.rightThree}"></a></li>
					</ol>
				</div>
			</div>
		</div>

		<div id="copyright">
			<p id="line1" class="text-center" th:text="#{label.footer.centerOne}"></p>
			<p id="line2" class="text-center" th:text="#{label.footer.centerTwo}"></p>
			<p id="line2" class="text-center" th:text="#{label.version.number}"></p>
		</div>
	</div>
</footer>