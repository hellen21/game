<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:th="http://www.thymeleaf.org"
	xmlns:sec="http://www.thymeleaf.org/thymeleaf-extras-springsecurity3">

	<head>
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<meta http-equiv="Cache-control" content="public" />
		
		<!-- ===========STYLES=========== -->
		<div th:include="layouts/styles" th:remove="tag"></div>
		
		<link th:href="@{/css/contact.css}" rel="stylesheet" media="screen" />
		<link rel="stylesheet" type="text/css" th:href="@{/css/jquery.realperson.css}" />
		<link th:href="@{/assets/bootstrapvalidator/bootstrapValidator.min.css}" rel="stylesheet" media="screen" />
		
		<title th:text="#{label.home.title}"></title>
	</head>
	<body>
	
		<!-- ===========HEADER=========== -->
		<div th:include="layouts/header" th:remove="tag"></div>
	
		<!-- ===========Carousel=========== -->
		<div class="container bodyContainer">
			<div class="container containerTemplete first">
				<div class="col-sm-7 ">
					<h1 th:text="#{label.contactus.title}"></h1>
					<h6>
						<strong th:text="#{label.contactus.subtitle}"></strong>
					</h6>
					<p th:text="#{label.contactus.description}"></p>
					<form class="form-horizontal" id="formContact">
						<div class="form-group">
							<div class="col-xs-12 col-sm-8">
								<div class="btn-group">
									<select  class="btn btn-default dropdown-toggle" data-toggle="dropdown" id="job" name="job">
										<!--  <option data-bind="label"  th:with="firstEmail=${jobsAndEmails[0]}"   th:text="${firstEmail.ContactType}"> 
										</option>-->
										<option  th:each="jobs : ${jobsAndEmails}" class="itemJob" th:text="${jobs.ContactType}" th:attr="data-code=${jobs.idContant}">
										</option>
									</select>
								</div>
							</div>
						</div>
						<div class="form-group">
							<div class="col-xs-12">
								<input type="text" class="form-control" placeholder="Nombre"
									id="name" name="name"  maxlength="100" autocomplete="off"></input> <span
									class="glyphicon glyphicon-user form-control-feedback"></span>
							</div>
						</div>
						<div class="form-group">
							<div class="col-xs-12">
								<input type="text" maxlength="100" class="form-control" placeholder="Empresa"
									id="factory" name="factory" autocomplete="off"></input> <span
									class="glyphicon glyphicon-briefcase form-control-feedback"></span>
							</div>
						</div>
						<div class="form-group">
							<div class="col-xs-12">
								<input type="text" maxlength="250" class="form-control" placeholder="Dirección"
									id="address" name="address" autocomplete="off"></input> <span
									class="glyphicon glyphicon-map-marker form-control-feedback"></span>
							</div>
						</div>
				<!-- 		<div class="form-group">
							<div class="col-xs-12 col-sm-8">
								<div class="btn-group">
	
									<button type="button" class="btn btn-default dropdown-toggle"
										data-toggle="dropdown" id="coutry" name="country">
										<span data-bind="label">Selección de País</span>&nbsp;<span
											class="caret"></span> <span
											class="glyphicon glyphicon-globe form-control-feedback"></span>
									</button>
									<ul class="dropdown-menu" role="menu">
										<li><a href="#">Costa Rica</a></li>
										<li><a href="#">Corea del Sur</a></li>
										<li><a href="#">Corea del Norte</a></li>
										<li><a href="#">Cuba</a></li>
									</ul>
	
								</div>
	
							</div>
						</div> -->
						<!-- <div class="form-group">
							<div class="col-xs-12 col-sm-8">
								<div class="btn-group">
									<button type="button" class="btn btn-default dropdown-toggle"
										data-toggle="dropdown" id="city" name="city">
										<span data-bind="label">Ciudad/Provincia</span>&nbsp;<span
											class="caret"></span> <span
											class="glyphicon glyphicon-flag form-control-feedback"></span>
									</button>
									<ul data-role="listview" data-inset="true" data-filter="true"
										data-filter-placeholder="Buscar"
										class="dropdown-menu provinces" role="menu">
									</ul>
	
								</div>
							</div>
						</div> -->
						<div class="form-group">
							<div class="col-xs-12">
								<input type="text" class="form-control" placeholder="Identificación"
									id="ced" name="ced" maxlength="30" autocomplete="off"></input> 
									<span class="glyphicon glyphicon-credit-card form-control-feedback"></span>
							</div>
						</div>
						<div class="form-group">
							<div class="col-xs-12">
								<input  maxlength="12" type="tel" class="form-control" placeholder="Teléfono"
									id="phone" name="phone" autocomplete="off"></input> <span
									class="glyphicon glyphicon-earphone form-control-feedback"></span>
							</div>
						</div>
						<div class="form-group">
							<div class="col-xs-12">
								<input type="email" maxlength="200" class="form-control" id="inputEmail"
									name="inputEmail" placeholder="E-mail" autocomplete="off"></input> <span
									class="glyphicon glyphicon-envelope form-control-feedback"></span>
							</div>
						</div>
						<div class="form-group">
							<div class="col-xs-12">
								<textarea rows="3" class="form-control" 
									placeholder="Mensaje/Comentarios" id="comment" name="comment" maxlength="2000" autocomplete="off"></textarea>
								<span class="glyphicon glyphicon-comment form-control-feedback"></span>
							</div>
						</div>
						<div class="form-group">
							<div class="col-xs-12">
								<canvas id="txtCaptcha" width="auto" height="75"
									style="border: 1px solid #d3d3d3;">
										</canvas>
	
								<a id="btnrefresh" onclick="DrawCaptcha();"><span
									class="glyphicon glyphicon-refresh form-control-feedback"></span></a>
								<input type="text" id="txtInput" name="txtInput"
									class="form-control" autocomplete="off" />
								
								<div id="snoAlertBox" class="alert alert-success"
									data-alert="alert" style="display: none" th:text="#{label.contact.CodeIncorrect}"></div>
	
								<!-- <input
									id="Button1" type="button" value="Check"
									onclick="alert(ValidCaptcha());" /> -->
							</div>
						</div>
						<div class="form-group">
							<input type="hidden" id="captchaConfirm" name="captchaConfirm"
									class="form-control" />
						</div>
						<div class="form-group">
							<div class="col-xs-12">
								<input type="submit" class="btn btn-primary" disabled="disabled" value="Enviar" />
							</div>
						</div>
					</form>
				</div>
			</div>
			<div class="container containerTemplete msjContact">
				<h1>
					<strong th:text="#{label.contactus.titlePage}"></strong>
				</h1>
				<h6>
					<strong th:text="#{label.contactus.thanks}"></strong>
				</h6>
				<p class="pThanks" th:text="#{label.contactus.confirm.mjs}"></p>
				<div class="buttonThanks">
					<input type="button" id="newMsj" value="Nuevo Mensaje"></input>
				</div>
			</div>
		</div>
	
		<div class="displayFooter">
			<div class="displayFooterArrow">
				<span class="glyphicon glyphicon-chevron-up"></span>
			</div>
		</div>
	
		<!-- ===========FOOTER=========== -->
		<div th:include="layouts/footer" th:remove="tag"></div>
		<!-- ===========SCRIPTS=========== -->
		<div th:include="layouts/scripts" th:remove="tag"></div>
		<script th:src="@{/assets/bootstrapvalidator/bootstrapValidator.js}"></script>
		<script type="text/javascript" th:src="@{/js/jquery.plugin.js}"></script>
		<script type="text/javascript" th:src="@{/js/jquery.realperson.js}"></script>
		<script type="text/javascript" th:src="@{/js/contact.js}"></script>
	<!-- 	<script th:src="@{/js/gAbalytics.js}"></script>
		<script th:src="@{/assets/jquery-storage/jquery.storageapi.min.js}"></script>
		<script th:src="@{/js/storageAPI.js}"></script>
		<script th:src="@{/js/utils.js}"></script>
		<script th:src="@{/js/nearme.js}"></script> -->
		<script th:src="@{/js/mutualBenefitst/benefict.js}"></script>
		<script th:inline="javascript">		
		 $(document).ready(function(){
			 contactMessage(document.getElementById("imgHeaderCompany").getAttribute("href"));	
			 DrawCaptcha();
			 addValidatorToContactForm();
		 });
			var company = [[${company}]];	
			var fooCarousel = [[${footerCarousel}]];		
			$('#fooCarouselDiv').replaceWith(fooCarousel);	
			
		</script>
	</body>
</html>